{
    "name": "tzsk/otp",
    "description": "OTP Generator and Verifier without database",
    "keywords": [
        "tzsk",
        "otp",
        "php otp",
        "laravel otp",
        "otp generator",
        "otp verification"
    ],
    "homepage": "https://github.com/tzsk/otp",
    "license": "MIT",
    "authors": [
        {
            "name": "tzsk",
            "email": "mailtokmahmed@gmail.com",
            "homepage": "https://github.com/tzsk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/cache": "^8.0|^9.0|^10.0",
        "illuminate/filesystem": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0",
        "laravel/pint": "^1.4",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Tzsk\\Otp\\": "src"
        },
        "files": [
            "src/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tzsk\\Otp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tzsk\\Otp\\OtpServiceProvider"
            ],
            "aliases": {
                "Otp": "Tzsk\\Otp\\Facades\\Otp"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
